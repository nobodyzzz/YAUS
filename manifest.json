{
	"name": "YAUS",
	"version": "0.0.3",
	"description": "Yet another url shortener extension",
	"browser_action": {
		"default_icon": "icon.png" 
	},
	"permissions": [
		"http://api.bit.ly/*",
		"http://api.tr.im/v1/*",
		"http://migre.me/*",
		"http://is.gd/*",
		"http://miud.in/*",
		"http://udanax.org/*",
		"http://urli.nl/*",
		"http://clck.ru/*",
        "https://www.googleapis.com/*",
		"tabs" 
	],
	"options_page": "options.html",
	"background_page": "background.html"
}
